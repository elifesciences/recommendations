{
    "name": "elife/recommendations",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\Recommendations\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "test\\eLife\\Recommendations\\": "test"
        }
    },
    "require": {
        "elife/api-client": "^1.0",
        "elife/api-problem": "^1.1",
        "elife/api-sdk": "dev-master",
        "elife/content-negotiator": "^1.0",
        "elife/logging-sdk": "^1.0@dev",
        "elife/ping": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/promises": "^1.3",
        "psr/log": "^1.0",
        "silexandfriends/silex": "^3.1",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "willdurand/negotiation": "^3.1"
    },
    "require-dev": {
        "csa/guzzle-cache-middleware": "^1.0",
        "csa/guzzle-stopwatch-middleware": "^1.0",
        "elife/api": "^2.30",
        "elife/api-validator": "^1.0@dev",
        "justinrainbow/json-schema": "^5.2",
        "phpunit/phpunit": "^9.2",
        "silexandfriends/web-profiler": "^3.1@DEV",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^5.4",
        "symfony/psr-http-message-bridge": "^2.0",
        "nyholm/psr7": "^1.1"
    },
    "config": {
        "platform": {
            "php": "8.3.21"
        },
        "sort-packages": true
    }
}
